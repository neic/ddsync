#!/bin/sh

LOGFILE="l2s-billeder.log"

source ~/ddsync/settings.txt

echo -e "\n\n" >> $LOGFILE
date +%Y-%m-%dT%H:%M:%S%z >> $LOGFILE
echo -e "\n\n" >> $LOGFILE

rsync -rltLzPhO "$DATAFOLDER/${USER^^} Privat" ${USER,,}@vae.danishdecoration.dk:/tank/dd/share/ | tee -a $LOGFILE

echo -e "\n\n" >> $LOGFILE

echo "Afsluttet. Venter i 20 sekunder"
sleep 20
